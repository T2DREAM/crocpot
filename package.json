{
  "name": "locuszoom",
  "version": "0.12.0-alpha.1",
  "main": "dist/locuszoom.app.min.js",
  "module": "esm/index.js",
  "sideEffects": true,
  "description": "Generate interactive visualizations of statistical genetic data",
  "keywords": [
    "visualization",
    "genetics",
    "genes",
    "gwas",
    "phewas"
  ],
  "homepage": "http://locuszoom.org",
  "license": "MIT",
  "author": "Christopher Clark <chriclrk@umich.edu>",
  "contributors": [
    "Matthew Flickinger",
    "Ryan Welch",
    "Peter VandeHaar",
    "Andy Boughton <abought@umich.edu>"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/statgen/locuszoom.git"
  },
  "scripts": {
    "test": "mocha --recursive test/**/*.js",
    "test:coverage": "nyc --reporter=text mocha --recursive test/**/*.js",
    "css": "sass ./css/locuszoom.scss ./dist/locuszoom.css",
    "dev": "npm run css && webpack --watch --config webpack.dev.cjs",
    "prod": "npm run css && webpack --config webpack.prod.cjs"
  },
  "dependencies": {
    "d3": "3.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "ignore-styles": "^5.0.1",
    "jsdoc": "^3.6.4",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "~7.2.0",
    "nyc": "^15.1.0",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "sinon": "^6.1.6",
    "source-map-loader": "^1.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
